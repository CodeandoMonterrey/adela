          %td 100%
          %td 15-nov-2015
          %td= link_to 'Editar', '#'
        %tr
          %td Niveles actuales de ríos
          %td 100%
          %td 15-nov-2015
          %td= link_to 'Editar', '#'
        %tr
          %td Temperatura actual por es...
          %td 100%
          %td 15-nov-2015
          %td= link_to 'Editar', '#'
    .row
      .col-xs-12.col-sm-4.navigation-back
        %p= link_to "< Atrás", '#'
      .col-xs-12.col-sm-4.navigation-center
        %p Mostrando 1 a 5 de 18 Resultados
      .col-xs-12.col-sm-4.navigation-right
        %p= link_to "Siguiente >", '#'
